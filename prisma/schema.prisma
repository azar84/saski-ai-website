model global_features {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  iconUrl     String?
  category    String?
  isActive    Boolean  @default(true)
  sortOrder   Int      @default(0)
  createdAt   DateTime @default(now())
  updatedAt   DateTime
}

model global_functions {
  id        Int      @id @default(autoincrement())
  functions String
  createdAt DateTime @default(now())
  updatedAt DateTime
}

model header_config {
  id               Int                @id @default(autoincrement())
  logoUrl          String?
  logoAlt          String?
  isActive         Boolean            @default(true)
  createdAt        DateTime           @default(now())
  updatedAt        DateTime
  backgroundColor  String?            @default("#ffffff")
  menuActiveColor  String?            @default("#5243E9")
  menuHoverColor   String?            @default("#5243E9")
  menuTextColor    String?            @default("#374151")
  HeaderConfigMenu HeaderConfigMenu[]
  header_nav_items header_nav_items[]
}

model header_ctas {
  id             Int      @id @default(autoincrement())
  headerConfigId Int
  ctaId          Int
  sortOrder      Int      @default(0)
  isVisible      Boolean  @default(true)
  createdAt      DateTime @default(now())
  updatedAt      DateTime
  ctas           ctas     @relation(fields: [ctaId], references: [id], onDelete: Cascade)
}

model header_nav_items {
  id             Int           @id @default(autoincrement())
  headerConfigId Int
  pageId         Int?
  label          String
  customText     String?
  customUrl      String?
  sortOrder      Int           @default(0)
  isVisible      Boolean       @default(true)
  createdAt      DateTime      @default(now())
  updatedAt      DateTime
  header_config  header_config @relation(fields: [headerConfigId], references: [id], onDelete: Cascade)
  pages          pages?        @relation(fields: [pageId], references: [id], onDelete: Cascade)
}

model header_sections {
  id              Int      @id @default(autoincrement())
  name            String
  backgroundColor String?  @default("#ffffff")
  menuTextColor   String?  @default("#374151")
  menuHoverColor  String?  @default("#5243E9")
  menuActiveColor String?  @default("#5243E9")
  isSticky        Boolean  @default(true)
  showLogo        Boolean  @default(true)
  showNavigation  Boolean  @default(true)
  showCTAs        Boolean  @default(true)
  isActive        Boolean  @default(true)
  createdAt       DateTime @default(now())
  updatedAt       DateTime
}

model hero_sections {
  id                                      Int      @id @default(autoincrement())
  name                                    String   @default("Untitled Hero Section")
  layoutType                              String   @default("split")
  sectionHeight                           String   @default("100vh")
  tagline                                 String?
  headline                                String
  subheading                              String?
  textAlignment                           String   @default("left")
  ctaPrimaryId                            Int?
  ctaSecondaryId                          Int?
  mediaUrl                                String?
  mediaType                               String   @default("image")
  mediaAlt                                String?
  mediaHeight                             String   @default("80vh")
  mediaPosition                           String   @default("right")
  backgroundType                          String   @default("color")
  backgroundValue                         String   @default("#FFFFFF")
  taglineColor                            String   @default("#5243E9")
  headlineColor                           String   @default("#1F2937")
  subheadingColor                         String   @default("#6B7280")
  ctaPrimaryBgColor                       String   @default("#5243E9")
  ctaPrimaryTextColor                     String   @default("#FFFFFF")
  ctaSecondaryBgColor                     String   @default("transparent")
  ctaSecondaryTextColor                   String   @default("#5243E9")
  showTypingEffect                        Boolean  @default(false)
  enableBackgroundAnimation               Boolean  @default(false)
  customClasses                           String?
  paddingTop                              Int      @default(80)
  paddingBottom                           Int      @default(80)
  containerMaxWidth                       String   @default("2xl")
  visible                                 Boolean  @default(true)
  createdAt                               DateTime @default(now())
  updatedAt                               DateTime
  ctas_hero_sections_ctaPrimaryIdToctas   ctas?    @relation("hero_sections_ctaPrimaryIdToctas", fields: [ctaPrimaryId], references: [id])
  ctas_hero_sections_ctaSecondaryIdToctas ctas?    @relation("hero_sections_ctaSecondaryIdToctas", fields: [ctaSecondaryId], references: [id])
}

model home_page_hero {
