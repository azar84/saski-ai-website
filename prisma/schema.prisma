  ctaSecondaryId   Int?
  ctaPrimaryText   String?
  ctaPrimaryUrl    String?
  ctaSecondaryText String?
  ctaSecondaryUrl  String?
  mediaUrl         String?
  backgroundColor  String?  @default("#FFFFFF")
  isActive         Boolean  @default(true)
