{
  "name": "saskiai-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && npm run db:migrate && next build",
    "db:migrate": "node scripts/migrate-production.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "preview": "next build && next start",
    "clean": "rm -rf .next",
    "analyze": "ANALYZE=true npm run build",
    "seed:postgres": "node seed-postgres.js",
    "seed:neon": "node seed-neon.js",
    "dev-setup": "node scripts/dev-setup.js",
    "setup-local-db": "node scripts/setup-local-db.js",
    "db:reset": "npx prisma db push --force-reset && node scripts/setup-local-db.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.10.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toast": "^1.2.14",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/nodemailer": "^6.4.17",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.18.1",
    "googleapis": "^150.0.1",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.522.0",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.3",
    "postcss": "^8.5.6",
    "prisma": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "react-icons": "^5.5.0",
    "react-spring": "^10.0.1",
    "sonner": "^2.0.5",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4.1.10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "next.js",
    "react",
    "typescript",
    "tailwindcss",
    "ai",
    "chatbot",
    "customer-service",
    "automation"
  ],
  "author": "Saski AI Team",
  "description": "Award-winning website for Saski AI - Transform your customer communication with AI"
}
